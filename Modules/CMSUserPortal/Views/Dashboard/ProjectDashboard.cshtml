        @Html.HiddenFor(model => model.Projects.sSync)
        @Html.HiddenFor(model => model.Projects.dtCreated)
        @Html.HiddenFor(model => model.Projects.dtUpdated)
        @Html.HiddenFor(model => model.Projects.dtViewed)
        @Html.HiddenFor(model => model.Projects.dtOpenned)
        @Html.HiddenFor(model => model.Projects.sBuildBaseFolder)
        @Html.HiddenFor(model => model.Projects.iConnectionTypeID)
        @Html.HiddenFor(model => model.Projects.sConnectionTypeID)
        @Html.HiddenFor(model => model.Projects.sConnectionString)
        @Html.HiddenFor(model => model.Projects.sConnUserName)
        @Html.HiddenFor(model => model.Projects.sConnUserPassword)
        @Html.HiddenFor(model => model.Projects.sDBLocation)
        @Html.HiddenFor(model => model.Projects.sSourceLocationForAutoUpdate)
        @Html.HiddenFor(model => model.Projects.sFileLocation)
        @Html.HiddenFor(model => model.Projects.iParentID)
        @Html.HiddenFor(model => model.Projects.sParentID)
        @Html.HiddenFor(model => model.Projects.iCreatedByID)
        @Html.HiddenFor(model => model.Projects.sCreatedByID)
        @Html.HiddenFor(model => model.Projects.iUpdatedByID)
        @Html.HiddenFor(model => model.Projects.sUpdatedByID)
        @Html.HiddenFor(model => model.Projects.iDomainID)
        @Html.HiddenFor(model => model.Projects.sDomainID)
        @Html.HiddenFor(model => model.Projects.dtLastBuild)


        <fieldset>
            <legend></legend>
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-lg-left text-md-left text-sm-left text-xs-left">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-lg-left text-md-left text-sm-left text-xs-left">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-lg-left text-md-left text-sm-left text-xs-left">
                        <div class="form-group">
                            <label for="Projects_sName">Name :</label>
                            @Html.TextBoxFor(model => model.Projects.sName, new { @Class = "form-control" })
                            @if (ViewBag.bError == true)
                            {
                                @Html.ValidationMessageFor(model => model.Projects.sName, "*Required!")
                            }
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 text-lg-left text-md-left text-sm-left text-xs-left">
                        <div class="form-group">
                            <label for="Projects_sDefaultNameSpace">Default Name Space :</label>
                            @Html.TextBoxFor(model => model.Projects.sDefaultNameSpace, new { @Class = "form-control" })
                            @if (ViewBag.bError == true)
                            {
                                @Html.ValidationMessageFor(model => model.Projects.sDefaultNameSpace, "*Required!")
                            }
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 text-lg-left text-md-left text-sm-left text-xs-left">
                        <div class="form-group">
                            <label for="Projects_sUINameSpace">UI Name Space :</label>
                            @Html.TextBoxFor(model => model.Projects.sUINameSpace, new { @Class = "form-control" })
                            @if (ViewBag.bError == true)
                            {
                                @Html.ValidationMessageFor(model => model.Projects.sUINameSpace, "*Required!")
                            }
                        </div>
                    </div>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-lg-left text-md-left text-sm-left text-xs-left">
                    <div class="form-group">
                        <label for="Projects_sDescription">Project Description :</label>
                        @Html.TextAreaFor(model => model.Projects.sDescription, new { @Class = "form-control" })
                        @if (ViewBag.bError == true)
                        {
                            @Html.ValidationMessageFor(model => model.Projects.sDescription, "*Required!")
                        }
                    </div>
                </div>
            </div>
        </fieldset>
        <div class="row">
            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4 text-lg-left text-md-left text-sm-left text-xs-left">
                &nbsp;
            </div>
            <div class="col-lg-7 col-md-7 col-sm-7 col-xs-7 text-lg-right text-md-right text-sm-right text-xs-right">
                <input type="submit" value="SAVE" name="submit" class="btn btn-outline-success" />
            </div>
        </div>

    }
</div>
