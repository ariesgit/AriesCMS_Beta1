@model DataView.ProjectsDBView

<script type="text/javascript">
    function On_NavRequest_ParentShowPleaseWait()
    {
        try{
            parent.Show_PleaseWait();
        }
        catch (e) {

        }
    }
</script>


<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 border_bottom_solid_1x">
        <div class="col-lg-7 col-md-7 col-sm-7 col-xs-7 text-lg-left text-md-left text-sm-left text-xs-left">
            <h3 class="text-bold"><a href="javascript:ShowHelp('100');"><img src="~/Themes/acev1/images/buttons/SymbolHelp16.png" alt="Show Help" /></a> Project Overview</h3>

            @if (ViewBag.bError == true)
            {
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 alert-danger">
                    <p>@Html.Raw(ViewBag.sErrorMessage)</p>
                </div>
            }
            else if (ViewBag.bSaved == true)
            {
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 alert-success">
                    <p>Your request was processed successfully.</p>
                </div>
            }
        </div>
        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3 text-lg-right text-md-right text-sm-right text-xs-right">
            <a href="#" class="btn btn-outline-success">PROMOTE YOUR PROJECT</a>
        </div>
    </div>
</div>
<div class="row">
    @using (Html.BeginForm("Projects_PRAddUpdate2", "", FormMethod.Post))
    {
        @Html.ValidationSummary(true);

        @Html.HiddenFor(model => model.Projects.ID)
